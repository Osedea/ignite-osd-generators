// @flow
import { createSelector } from 'reselect';

/**
 * Direct selector to the <%= props.name %> state domain
 */
const select<%= props.name %>State = () => (state) => state.get('<%= props.name %>');

export const select<%= props.name %>List = () =>
    createSelector(select<%= props.name %>State(), (substate) => substate.get('list'));

export const selectErrors = () =>
    createSelector(select<%= props.name %>State(), (substate) => substate.get('errors'));
