import { createSelector } from 'reselect';

/**
 * Direct selector to the <%= props.name %> state domain
 */
const select<%= props.name %> = () => (state) => state.<%= props.name %>;

export const select<%= props.name %>List = () =>
    createSelector(select<%= props.name %>(), (substate) => substate.list);

export const selectErrors = () =>
    createSelector(select<%= props.name %>(), (substate) => substate.errors);
