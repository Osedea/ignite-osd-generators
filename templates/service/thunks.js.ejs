import RequestHandler from '<%= props.appName %>/app/helpers/requestHandler';
import { fetch<%= props.name %>Try, fetch<%= props.name %>Success, fetch<%= props.name %>Fail } from './actions';

/**
 * GET <%= props.name %>
 */
export const get<%= props.name %> = () => (dispatch: Function) => {
    return RequestHandler.requestAPI(`/<%= props.name.toLowerCase() %>`);
        .then((data) => {
            dispatch(got<%= props.name %>s(data));
        })
        .catch((response) => {
            dispatch(get<%= props.name %>sFailed(response.message));
        });
};
